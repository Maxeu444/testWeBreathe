{% extends 'base.html.twig' %} {% block title %}Module monitoring - Liste{%
endblock %} {% block body %}


<div id="tableContainer">
  <h2 class="sectionTitle">Liste des modules</h2>
  <table id="moduleTable" class="table table-light table-striped rounded-6">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Type</th>
        <th scope="col">Description</th>
        <th scope="col">Capaciti√©</th>
      </tr>
    </thead>
    <tbody>
      {% for donnee in donnees %}
      <tr>
